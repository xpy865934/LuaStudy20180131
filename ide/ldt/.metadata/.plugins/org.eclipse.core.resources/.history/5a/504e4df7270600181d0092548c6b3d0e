function clone(tab)
    local ins = {}
    for key, var in pairs(tab) do
    	ins[key] = var
    end
    return ins	
end


People  = {}

--定义对象的方法
--1、
--function People.sayHi()
--	print("Say Hi")
--end
--2、
People.sayHi = function (self)
    print("Say Hi "..self.name)  
end

People.new = function (name)
	local self = clone(People)
	self.name = name
	return self
end

--local p = clone(People)
--p.sayHi()

local p1 = People.new("zhangsan")
p1.sayHi(p1)